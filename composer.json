{
  "name": "fluxlabs/fluxopenidconnectapi",
  "description": "Open Id Connect",
  "type": "project",
  "keywords": [
    "connect",
    "id",
    "open"
  ],
  "homepage": "https://github.com/fluxapps/FluxOpenIdConnectApi",
  "license": "GPL-3.0-only",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fluxapps/FluxRestApi"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-swoole": "*",
    "fluxlabs/fluxrestapi": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "FluxOpenIdConnectApi\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
